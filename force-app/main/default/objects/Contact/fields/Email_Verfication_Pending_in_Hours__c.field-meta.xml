<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Email_Verfication_Pending_in_Hours__c</fullName>
    <description>Number of hours it took from &quot;First Verification Email Sent&quot; to &quot;Email verified&quot;</description>
    <externalId>false</externalId>
    <formula>(FLOOR(( EmailVerificationDateTime__c - Email_Verification_Sent_DateTime__c )*24))</formula>
    <label>Email Verification Pending in Hours</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
